{
  "name": "@jupyrdf/jupyter-elk",
  "version": "0.3.0",
  "description": "ElkJS widget for Jupyter",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension",
    "widgets"
  ],
  "homepage": "https://github.com/jupyrdf/ipyelk",
  "bugs": {
    "url": "https://github.com/jupyrdf/ipyelk/issues"
  },
  "license": "BSD-3-Clause",
  "author": "Dane Freeman",
  "files": [
    "{lib,style,src}/**/*.{.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf,css}",
    "LICENSE.txt",
    "COPYRIGHT.md",
    "third-party/**/*"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "repository": {
    "type": "git",
    "url": "https://github.com/jupyrdf/ipyelk"
  },
  "prettier": {
    "singleQuote": true,
    "proseWrap": "always",
    "printWidth": 88
  },
  "scripts": {
    "bootstrap": "jlpm --ignore-optional --ignore-scripts && jlpm clean && jlpm schema && jlpm lint && jlpm build",
    "build": "tsc -b",
    "clean": "rimraf lib ./py_src/ipyelk/schema/elkschema.json",
    "watch": "jlpm build -w --preserveWatchOutput",
    "schema": "ts-json-schema-generator --tsconfig tsconfig.json --path ./src/elkschema.ts -o ./py_src/ipyelk/schema/elkschema.json",
    "lint": "jlpm lint:prettier",
    "lint:prettier": "prettier --write --list-different \"*.{json,yml,md}\"  \"{src,style,py_src,.github,examples}/**/*.{ts,tsx,js,jsx,css,json,md,yml}\""
  },
  "dependencies": {
    "reflect-metadata": "^0.1.13",
    "sprotty-elk": "0.9.0",
    "worker-loader": "^2.0.0"
  },
  "devDependencies": {
    "@jupyter-widgets/base": "^2.0.1",
    "@jupyter-widgets/controls": "^1.5.1",
    "@jupyter-widgets/jupyterlab-manager": "^1.0.1",
    "@jupyterlab/application": "^1.2.9",
    "@types/d3": "^5.16.3",
    "@types/lodash": "^4.14.162",
    "prettier": "^1.9.1",
    "react": "*",
    "rimraf": "^2.6.1",
    "snabbdom": "~0.6.6",
    "ts-json-schema-generator": "^0.71.2",
    "typescript": "~3.9.2"
  },
  "peerDependencies": {
    "@jupyterlab/application": "^1.2.7",
    "@jupyter-widgets/base": "^2.0.1",
    "@jupyter-widgets/controls": "^1.5.1",
    "@jupyter-widgets/jupyterlab-manager": "^1.0.1",
    "react": "*"
  },
  "sideEffects": [
    "style/*.css"
  ],
  "jupyterlab": {
    "extension": "lib/plugin"
  }
}
